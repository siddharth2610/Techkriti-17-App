<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.suleiman.techkriti.activities.RegEmbed"
    tools:showIn="@layout/activity_reg_embed">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/katy"
            android:gravity="center_horizontal"
            android:textColor="#ffffffff"
            android:textColorHint="#ffffffff"
            android:layout_marginTop="10dp"
            android:textSize="24sp"
            android:layout_gravity="center_horizontal" />
        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginTop="20dp"
            android:orientation="vertical">


                <EditText
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/teamname"
                    android:layout_marginStart="9dp"
                    android:textColor="#ffffffff"
                    android:textColorHint="#ffffffff"
                    android:layout_marginLeft="9dp"
                    android:layout_marginTop="10dp"
                    android:hint="Team Name"/>
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Team member 1"
                    android:id="@+id/text1"
                    android:layout_marginStart="6dp"
                    android:layout_marginLeft="6dp"
                    android:textColor="#ffffffff"
                    android:textColorHint="#ffffffff"
                    android:layout_marginTop="7dp"
                    android:textSize="20sp"/>
                <EditText
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/num1"
                    android:textColor="#ffffffff"
                    android:textColorHint="#ffffffff"
                    android:layout_marginStart="9dp"
                    android:layout_marginLeft="9dp"
                    android:hint="TECH id"/>
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Team member 2"
                    android:id="@+id/text2"
                    android:textColor="#ffffffff"
                    android:textColorHint="#ffffffff"
                    android:layout_marginStart="6dp"
                    android:layout_marginLeft="6dp"
                    android:layout_marginTop="7dp"
                    android:textSize="20sp"/>
                <EditText
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/num2"
                    android:textColor="#ffffffff"
                    android:textColorHint="#ffffffff"
                    android:layout_marginStart="9dp"
                    android:layout_marginLeft="9dp"
                    android:hint="TECH id"/>
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Team member 3"
                    android:textColor="#ffffffff"
                    android:textColorHint="#ffffffff"
                    android:id="@+id/text3"
                    android:layout_marginStart="6dp"
                    android:layout_marginLeft="6dp"
                    android:layout_marginTop="7dp"
                    android:textSize="20sp"/>
                <EditText
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/num3"
                    android:textColor="#ffffffff"
                    android:textColorHint="#ffffffff"
                    android:layout_marginStart="9dp"
                    android:layout_marginLeft="9dp"
                    android:hint="TECH id"/>
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Team member 4"
                    android:id="@+id/text4"
                    android:textColor="#ffffffff"
                    android:textColorHint="#ffffffff"
                    android:layout_marginStart="6dp"
                    android:layout_marginLeft="6dp"
                    android:layout_marginTop="7dp"
                    android:textSize="20sp"/>
                <EditText
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/num4"
                    android:textColor="#ffffffff"
                    android:textColorHint="#ffffffff"
                    android:layout_marginStart="9dp"
                    android:hint="TECH id"/>
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Team member 5"
                    android:id="@+id/text5"
                    android:textColor="#ffffffff"
                    android:textColorHint="#ffffffff"
                    android:layout_marginStart="6dp"
                    android:layout_marginLeft="6dp"
                    android:layout_marginTop="7dp"
                    android:textSize="20sp"/>
                <EditText
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/num5"
                    android:layout_marginStart="9dp"
                    android:layout_marginLeft="9dp"
                    android:hint="TECH id"/>
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Team member 6"
                    android:textColor="#ffffffff"
                    android:textColorHint="#ffffffff"
                    android:id="@+id/text6"
                    android:layout_marginStart="6dp"
                    android:layout_marginLeft="6dp"
                    android:layout_marginTop="7dp"
                    android:textSize="20sp"/>
                <EditText
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/num6"
                    android:textColor="#ffffffff"
                    android:textColorHint="#ffffffff"
                    android:layout_marginStart="9dp"
                    android:layout_marginLeft="9dp"
                    android:hint="TECH id"/>
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Team member 7"
                    android:textColor="#ffffffff"
                    android:textColorHint="#ffffffff"
                    android:id="@+id/text7"
                    android:layout_marginStart="6dp"
                    android:layout_marginLeft="6dp"
                    android:layout_marginTop="7dp"
                    android:textSize="20sp"/>
                <EditText
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/num7"
                    android:textColor="#ffffffff"
                    android:textColorHint="#ffffffff"
                    android:layout_marginStart="9dp"
                    android:layout_marginLeft="9dp"
                    android:hint="TECH id"/>
            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/register"
                android:text="Register"
                android:textColor="#ffffffff"
                android:background="#ff7e51c2"
                android:layout_marginStart="9dp"
                android:layout_marginLeft="100dp"
               />

        </LinearLayout>
    </ScrollView>
    </LinearLayout>

</LinearLayout>
